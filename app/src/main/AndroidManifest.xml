<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stawishaloans.manu.stawishaloans">
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application

        android:hardwareAccelerated="false"
        android:largeHeap="true"
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_r"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme"
        android:fullBackupContent="@xml/backup_descriptor">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/app_id" />
        <activity android:name="com.stawishaloans.manu.stawishaloans.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.stawishaloans.manu.stawishaloans.StartActivity" />
        <activity android:name="com.stawishaloans.manu.stawishaloans.GetStartActivity" />
        <activity android:name="com.stawishaloans.manu.stawishaloans.SignIn_SignUp"
            android:excludeFromRecents="true"
            android:windowSoftInputMode="adjustPan"
           >

        </activity>
        <activity android:name="com.stawishaloans.manu.stawishaloans.PurposeOfLoan" />
        <activity android:name="com.stawishaloans.manu.stawishaloans.LoanDetail" />
        <activity android:name="com.stawishaloans.manu.stawishaloans.LoanRequestReceived" />
        <activity android:name="com.stawishaloans.manu.stawishaloans.AlreadyAccount" />
        <activity android:name="com.stawishaloans.manu.stawishaloans.Refer_earn" />
        <activity android:name="com.stawishaloans.manu.stawishaloans.Faq" />
        <activity android:name="com.stawishaloans.manu.stawishaloans.ForgotPasword" />
        <activity android:name="com.stawishaloans.manu.stawishaloans.OptVerify" />
        <activity android:name="com.stawishaloans.manu.stawishaloans.NewPassword"/>
        <activity android:name="com.stawishaloans.manu.stawishaloans.TermAndCondition"/>
    </application>

</manifest>